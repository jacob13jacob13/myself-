def Quick_Sort(a):
    Smaller=[]
    Equal=[]
    Bigger=[]
     
    if len(a)<1:
        return a
    
    else:
        pivot=a[0] 
   
        
        for i in a:
            if i<pivot:
                Smaller.append(i)
            elif i==pivot:
                Equal.append(i)
            else:
                Bigger.append(i)
   
    S=Quick_Sort(Smaller)
    B=Quick_Sort(Bigger)
    E=Equal
        
    return S+E+B
